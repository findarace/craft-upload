{
    "name": "findarace/craft-upload",
    "description": "Front-end asset upload for user photos, fields, volumes and folders.",
    "type": "craft-plugin",
    "version": "4.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "asset uploader",
        "front end",
        "uploadit"
    ],
    "support": {
        "docs": "https://github.com/findarace/craft-upload/blob/master/README.md",
        "issues": "https://github.com/findarace/craft-upload/issues"
    },
    "license": "proprietary",
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "findarace\\upload\\": "src/"
        }
    },
    "extra": {
        "name": "Upload",
        "handle": "upload",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/findarace/craft-upload/master/CHANGELOG.md",
        "class": "findarace\\upload\\Upload",
        "components": {
            "upload": "findarace\\upload\\services\\UploadService"
        }
    }
}
